{
  "buildCommand": "cd simple-preview && npm run build && ls -la && pwd",
  "outputDirectory": "simple-preview/dist",
  "installCommand": "cd simple-preview && npm install",
  "devCommand": "cd simple-preview && npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}