{
  "buildCommand": "cd simple-preview && npm run build && cd .. && cp -r simple-preview/dist ./dist",
  "outputDirectory": "dist",
  "installCommand": "cd simple-preview && npm install",
  "devCommand": "cd simple-preview && npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}